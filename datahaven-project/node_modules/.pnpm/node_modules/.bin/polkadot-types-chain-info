#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/user/zamanovember/datahaven-project/node_modules/.pnpm/@polkadot+typegen@16.5.4/node_modules/@polkadot/typegen/scripts/node_modules:/home/user/zamanovember/datahaven-project/node_modules/.pnpm/@polkadot+typegen@16.5.4/node_modules/@polkadot/typegen/node_modules:/home/user/zamanovember/datahaven-project/node_modules/.pnpm/@polkadot+typegen@16.5.4/node_modules/@polkadot/node_modules:/home/user/zamanovember/datahaven-project/node_modules/.pnpm/@polkadot+typegen@16.5.4/node_modules:/home/user/zamanovember/datahaven-project/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/user/zamanovember/datahaven-project/node_modules/.pnpm/@polkadot+typegen@16.5.4/node_modules/@polkadot/typegen/scripts/node_modules:/home/user/zamanovember/datahaven-project/node_modules/.pnpm/@polkadot+typegen@16.5.4/node_modules/@polkadot/typegen/node_modules:/home/user/zamanovember/datahaven-project/node_modules/.pnpm/@polkadot+typegen@16.5.4/node_modules/@polkadot/node_modules:/home/user/zamanovember/datahaven-project/node_modules/.pnpm/@polkadot+typegen@16.5.4/node_modules:/home/user/zamanovember/datahaven-project/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@polkadot/typegen/scripts/polkadot-types-chain-info.mjs" "$@"
else
  exec node  "$basedir/../@polkadot/typegen/scripts/polkadot-types-chain-info.mjs" "$@"
fi
